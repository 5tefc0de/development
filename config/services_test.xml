<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="true" />

        <service class="Shopware\Core\Checkout\Test\Payment\Handler\TestPaymentHandler" id="Shopware\Core\Checkout\Test\Payment\Handler\TestPaymentHandler">
            <argument type="service" id="order_transaction.repository" />
            <tag name="payment.method" />
        </service>

        <service class="Shopware\Core\Framework\Test\Filesystem\Adapter\MemoryAdapterFactory" id="Shopware\Core\Framework\Test\Filesystem\Adapter\MemoryAdapterFactory">
            <tag name="Shopware\Core\Framework\Filesystem\FilesystemFactory" />
        </service>

        <service class="Shopware\Core\Framework\Test\ORM\Field\TestDefinition\NamedDefinition" id="Shopware\Core\Framework\Test\ORM\Field\TestDefinition\NamedDefinition">
            <tag name="shopware.entity.definition" entity="named"/>
        </service>

        <service class="Shopware\Core\Framework\Test\ORM\Field\TestDefinition\NamedOptionalGroupDefinition" id="Shopware\Core\Framework\Test\ORM\Field\TestDefinition\NamedOptionalGroupDefinition">
            <tag name="shopware.entity.definition" entity="named_optional_group"/>
        </service>

        <service id="named.repository" class="Shopware\Core\Framework\ORM\EntityRepository" public="true">
            <argument>Shopware\Core\Framework\Test\ORM\Field\TestDefinition\NamedDefinition</argument>
            <argument type="service" id="Shopware\Core\Framework\ORM\Read\EntityReaderInterface"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\VersionManager"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\Search\EntitySearcherInterface"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\Search\EntityAggregatorInterface"/>
            <argument type="service" id="event_dispatcher"/>
        </service>

        <service id="named_optional_group.repository" class="Shopware\Core\Framework\ORM\EntityRepository" public="true">
            <argument>Shopware\Core\Framework\Test\ORM\Field\TestDefinition\NamedOptionalGroupDefinition</argument>
            <argument type="service" id="Shopware\Core\Framework\ORM\Read\EntityReaderInterface"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\VersionManager"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\Search\EntitySearcherInterface"/>
            <argument type="service" id="Shopware\Core\Framework\ORM\Search\EntityAggregatorInterface"/>
            <argument type="service" id="event_dispatcher"/>
        </service>
    </services>

</container>